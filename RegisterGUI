    private void ProductSaleSearchKeyPressed(java.awt.event.KeyEvent evt) {                                             
        if (evt.getKeyCode() == evt.VK_ENTER) {

            String PInput = ProductSaleSearch.getText();
            String quantity = saleQuantity.getText();
            register.addItemToSale(PInput, quantity);
            
            saleTrans.add ("insert into SOFTWARE_sale_ITEMS values ('" + PInput + "', '" + sale.getId() +"', '" + quantity + "')");
            
            itemNumSale++;
            updateSaleTable();
            
            
            
            
        }


    }                                            

    //This is the quantity code
    //Preconditions: We will have a product code entered into the product search
    //Postconditions: This will call the enter sale procedure (n-1) times
    //Its (n-1) because we will have already entered in one of the quantCount
    ////////////////////////////////////////////////////////////////////////////
    //all that is out
    //OKAY SO this is identical to ProductCodeSearchKeyPressed FOR NOW because
    //pressing enter in either field should have the same effect. CHANGE THIS LATER but for now it's okay!!
    private void saleQuantityKeyPressed(java.awt.event.KeyEvent evt) {                                        
        if (evt.getKeyCode() == evt.VK_ENTER) {

             String PInput = ProductSaleSearch.getText();
            String quantity = saleQuantity.getText();
            register.addItemToSale(PInput, quantity);
            itemNumSale++;
            updateSaleTable();
            
          

        }
    }                                       
//This is the finish Sale Button Mouse Clicked code
    private void finishSaleButtonMouseClicked(java.awt.event.MouseEvent evt) {                                              
        DBConnect db = new DBConnect();
        
        for (int i = 0; i < saleTrans.size(); i++){
            System.out.println(saleTrans.get(i));
        db.update(saleTrans.get(i));
        }
        register.finishSale();
        

    }                                             

    private void clearLastSaleClicked(java.awt.event.MouseEvent evt) {                                      
        // TODO add your handling code here:
        
        String PInput = ProductSaleSearch.getText();

       register.removeLastItemSale();
       itemNumSale--;
       updateSaleTable();
        
        
        
        
       
    }                                     

    private void cancelSaleButtonPressed(java.awt.event.MouseEvent evt) {                                         
        // TODO add your handling code here:
        
        register.cancelSale();
        itemNumSale = 0;
        updateSaleTable();

    }                                        

    private void returnQuantityKeyPressed(java.awt.event.KeyEvent evt) {                                          
        // TODO add your handling code here:
        if (evt.getKeyCode() == evt.VK_ENTER) {

            //float subtot = 0;
            String PInput = ProductSearchReturn.getText();
            String quantity = returnQuantity.getText();
            register.addItemToReturn(PInput, quantity);
            itemNumReturn++;
             updateReturnTable();

           // Subtotal1.setText("" + register.getReturnSubtotal() + "");
        }
          
        

    }                                         

    private void finishReturnButtonMouseClicked(java.awt.event.MouseEvent evt) {                                                
        // TODO add your handling code here:
        
        
        register.finishReturn();
      
    }                                               

