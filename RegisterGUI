    private void ProductCodeSaleKeyPressed(java.awt.event.KeyEvent evt) {                                           
        if (evt.getKeyCode() == evt.VK_ENTER) {

            String PInput = ProductCodeSale.getText();
            String quantity = saleQuantity.getText();
            register.addItemToSale(PInput, quantity);
            
            saleTrans.add ("insert into SOFTWARE_sale_ITEMS values ('" + PInput + "', '" + sale.getId() +"', '" + quantity + "')");
            
            itemNumSale = register.sale.getSize();
            updateSaleTable();
        }
    }                                          

    private void saleQuantityKeyPressed(java.awt.event.KeyEvent evt) {                                        
        if (evt.getKeyCode() == evt.VK_ENTER) {
            String PInput = ProductCodeSale.getText();
            String quantity = saleQuantity.getText();
            register.addItemToSale(PInput, quantity);
            
            saleTrans.add ("insert into SOFTWARE_sale_ITEMS values ('" + PInput + "', '" + sale.getId() +"', '" + quantity + "')");
            
            itemNumSale = register.sale.getSize();
            updateSaleTable();
        }
    }                                       

    private void finishSaleButtonMouseClicked(java.awt.event.MouseEvent evt) {                                              
        DBConnect db = new DBConnect();
        for (int i = 0; i < saleTrans.size(); i++){
            System.out.println(saleTrans.get(i));
        db.update(saleTrans.get(i));
        }
        register.finishSale();
        itemNumSale = 0;
        updateSaleTable();
    }                                             

    private void saleDeleteItemMouseClicked(java.awt.event.MouseEvent evt) {                                            
      
        register.removeItemSale(saleTable.getSelectedRow());
        itemNumSale--;
        updateSaleTable();
    }                                           

    private void cancelSaleButtonPressed(java.awt.event.MouseEvent evt) {                                         
        register.cancelSale();
        itemNumSale = 0;
        updateSaleTable();
    }                                        

    private void returnQuantityKeyPressed(java.awt.event.KeyEvent evt) {                                          
        if (evt.getKeyCode() == evt.VK_ENTER) {
            String PInput = ProductCodeReturn.getText();
            String quantity = returnQuantity.getText();
            register.addItemToReturn(PInput, quantity);
            itemNumReturn++;
            updateReturnTable();
        }
    }                                         

    private void finishReturnButtonMouseClicked(java.awt.event.MouseEvent evt) {                                                
        register.finishReturn();      
        updateReturnTable();
    }                                               

    private void cancelReturnButtonPressed(java.awt.event.MouseEvent evt) {                                           
        register.cancelReturn();
        itemNumReturn = 0;
        updateReturnTable();
    }                                          

    private void finishRentalButtonMouseClicked(java.awt.event.MouseEvent evt) {                                                
        register.finishRental();
        updateRentalTable();
    }                                               

    
    
    
    
    private void AddRental(java.awt.event.ActionEvent evt) {                           
        new RentalForm("" + Rent_id + "", RentCost, rentalTrans, register).setVisible(true);
    }                          

    
    
    
    
    
    private void productRentalSearch(java.awt.event.KeyEvent evt) {                                     
        if (evt.getKeyCode() == evt.VK_ENTER) {
             System.out.println("Enter");
            //float subtot = 0;
            String PInput = ProductCodeRental.getText();
            System.out.println(PInput);
            String quantity = rentalQuantity.getText();
            System.out.println("q: " + quantity);
            
            
            register.addItemToRental(PInput, quantity);
            
            
            //add item to rental_database
            DBConnect db = new DBConnect();
            //db.update("insert into SOFTWARE_RENTAL_ITEMS values ('" + PInput + "', '" + Rent_id + "')");
            System.out.println("insert into SOFTWARE_RENTAL_ITEMS values ('" + Rent_id + "', '" + PInput +"', '" + quantity + "');");
            rentalTrans.add ("insert into SOFTWARE_RENTAL_ITEMS values ('" + PInput + "', '" + Rent_id +"', '" + quantity + "')");
            
            
            itemNumRental++;
            updateRentalTable();
            RentCost = "" + register.getRentalSubtotal() + "";
            //SubtotalRental.setText("" + register.getRentalSubtotal() + "");
        }
        
        //System.out.println("Rental Worked!");
    }                                    

    private void rentalQuantityKeyPressed(java.awt.event.KeyEvent evt) {                                          
        // TODO add your handling code here:
    }                                         

    private void returnIDInput(java.awt.event.ActionEvent evt) {                               
        String status = RentalStatus.getText();
        String id = returnID.getText();
        DBConnect db = new DBConnect();
        //System.out.println("select * from software_rental_form where UPC = '" + id + "'" );
        if(
        db.query("select * from software_rental_form where Form_ID = '" + id + "'" ))
        {
            RentalStatus.setText("Rental Status: VALID RENTAL");
        }
        else{
            RentalStatus.setText("Rental Status: INVALID RENTAL");
        }
        
    }                              

    private void ProductCodeReturnKeyPressed(java.awt.event.KeyEvent evt) {                                             
        if (evt.getKeyCode() == evt.VK_ENTER){
            String PInput = ProductCodeReturn.getText();
            String quantity = returnQuantity.getText();
            register.addItemToReturn(PInput, quantity);

            itemNumReturn++;
            updateReturnTable();
        }
    }                                            

    private void receiptIDKeyPressed(java.awt.event.KeyEvent evt) {                                     
        if (evt.getKeyCode() == evt.VK_ENTER){
            String status = RentalStatus1.getText();
  
        String id = receiptID.getText();
        DBConnect db = new DBConnect();
        //System.out.println("select * from software_rental_form where UPC = '" + id + "'" );
        if(
        db.query("select * from software_sale_items where sale_ID = '" + id + "'" ))
        {
            RentalStatus1.setText("Return Status: VALID");
        }
        else{
            RentalStatus1.setText("Return Status: INVALID");
        }
        }
        
    }                                    

    private void saleTableMouseClicked(java.awt.event.MouseEvent evt) {                                       
       model = (DefaultTableModel)saleTable.getModel();
        
    }                                      

    private void saleEditButtonActionPerformed(java.awt.event.ActionEvent evt) {                                               

        
            String PInput = String.valueOf(model.getValueAt(saleTable.getSelectedRow(), 0));
            String quantity = String.valueOf(model.getValueAt(saleTable.getSelectedRow(), 2));
            register.editItemSale(PInput, quantity, saleTable.getSelectedRow());
            
            saleTrans.add ("insert into SOFTWARE_sale_ITEMS values ('" + PInput + "', '" + sale.getId() +"', '" + quantity + "')");
            
            itemNumSale = register.sale.getSize();
            updateSaleTable();
        
    }                                              

    private void returnEditButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        
        
        
            String PInput = String.valueOf(model.getValueAt(returnTable.getSelectedRow(), 0));
            String quantity = String.valueOf(model.getValueAt(returnTable.getSelectedRow(), 2));
            register.editItemReturn(PInput, quantity, returnTable.getSelectedRow());
            
            
            itemNumReturn = register.ret.getSize();
            updateReturnTable();
    }                                                

    private void rentalEditButtonActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        
         String PInput = String.valueOf(model.getValueAt(rentalTable.getSelectedRow(), 0));
            String quantity = String.valueOf(model.getValueAt(rentalTable.getSelectedRow(), 2));
            register.editItemRental(PInput, quantity, rentalTable.getSelectedRow());
            
            
            itemNumRental = register.rental.getSize();
            updateRentalTable();
            RentCost = "" + register.getRentalSubtotal() + "";
        
        
    }                                                

    private void returnDeleteItemActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        
        register.removeItemReturn(returnTable.getSelectedRow());
        itemNumReturn--;
        updateReturnTable();
    }                                                

    private void rentalCancelActionPerformed(java.awt.event.ActionEvent evt) {                                             
         register.cancelRental();
        itemNumRental = 0;
        RentCost = "" + register.getRentalSubtotal() + "";
        updateRentalTable();
    }                                            

    private void rentalDeleteItemActionPerformed(java.awt.event.ActionEvent evt) {                                                 
       
        register.removeItemRental(rentalTable.getSelectedRow());
        itemNumRental--;
        updateRentalTable();
    }                                                

    private void returnTableMouseClicked(java.awt.event.MouseEvent evt) {                                         
       
        model = (DefaultTableModel)returnTable.getModel();
    }                                        

    private void rentalTableMouseClicked(java.awt.event.MouseEvent evt) {                                         
        
        model = (DefaultTableModel)rentalTable.getModel();
    }                                        
    
   
    
    
    
    
    
    
